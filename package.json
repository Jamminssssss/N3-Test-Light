{
  "name": "N3TestApp",
  "version": "0.0.1",
  "private": true,
"scripts": {
  "dev": "concurrently \"npm run android\" \"react-native run-ios --simulator='iPhone SE (3rd generation)'\"",
  "android": "react-native run-android",
  "ios": "react-native run-ios",
  "lint": "eslint .",
  "start": "react-native start",
  "test": "jest",
  "clean": "watchman watch-del-all && rm -rf node_modules/ && npm cache clean --force && npm install",
  "build:android": "cd android && ./gradlew assembleRelease",
  "build:ios": "xcodebuild -workspace ios/YourApp.xcworkspace -scheme YourApp -configuration Release",
  "start:both": "concurrently \"npm run android\" \"npm run ios\"",
  "emulator": "emulator -list-avds && emulator -avd <Your-AVD-Name>",
  "upgrade": "npx react-native upgrade",
  "tsc": "tsc --noEmit",
  "apk": "cd android && ./gradlew assembleDebug",
  "bundle-visualizer": "react-native bundle --platform ios --dev false --entry-file index.js --bundle-output ios/main.jsbundle --assets-dest ios",
  "expo": "expo start"
},
  "dependencies": {
    "@react-native-vector-icons/ionicons": "^7.4.0",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "prop-types": "^15.8.1",
    "react": "19.0.0",
    "react-native": "0.78.0",
    "react-native-gesture-handler": "^2.24.0",
    "react-native-google-mobile-ads": "^14.9.1",
    "react-native-image-pan-zoom": "^2.1.12",
    "react-native-reanimated": "^3.17.0",
    "react-native-safe-area-context": "^5.2.0",
    "react-native-screens": "^4.9.1",
    "react-native-sound": "^0.11.2",
    "react-native-splash-screen": "^3.3.0",
    "react-native-sqlite-storage": "^6.0.1",
    "react-native-vector-icons": "^10.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "15.0.1",
    "@react-native-community/cli-platform-android": "15.0.1",
    "@react-native-community/cli-platform-ios": "15.0.1",
    "@react-native/babel-preset": "0.78.0",
    "@react-native/eslint-config": "0.78.0",
    "@react-native/metro-config": "0.78.0",
    "@react-native/typescript-config": "0.78.0",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.0.0",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/react-test-renderer": "^19.0.0",
    "concurrently": "^9.1.2",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "19.0.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
